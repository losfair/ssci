{
  "compilerOptions": {
    "outDir": "build",
    "target": "es2022",
    "module": "es2022",
    "allowJs": true,
    "esModuleInterop": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "moduleResolution": "node",
    "allowImportingTsExtensions": true,
    "noEmit": true
  }
}
